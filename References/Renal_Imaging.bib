  number = {6620},
  pmcid = {PMC2545041}
}

@article{drey_population-based_2003,
  title = {A Population-Based Study of the Incidence and Outcomes of Diagnosed Chronic Kidney Disease},
  author = {Drey, Nicholas and Roderick, Paul and Mullee, Mark and Rogerson, Mary},
  date = {2003-10-01},
  journaltitle = {American Journal of Kidney Diseases},
  shortjournal = {American Journal of Kidney Diseases},
  volume = {42},
