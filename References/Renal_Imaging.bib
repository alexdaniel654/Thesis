@article{hueper_diffusion_2011,
  title = {Diffusion Tensor Imaging and Tractography for Assessment of Renal Allograft Dysfunction\textemdash{}Initial Results},
  author = {Hueper, Katja and Gutberlet, M. and Rodt, T. and Gwinner, W. and Lehner, F. and Wacker, F. and Galanski, M. and Hartung, D.},
