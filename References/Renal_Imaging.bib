@article{chong_cerebral_2015,
  title = {Cerebral Metabolic Rate of Oxygen ({{CMRO2}}) Assessed by Combined {{Doppler}} and Spectroscopic {{OCT}}},
  author = {Chong, Shau Poh and Merkle, Conrad W. and Leahy, Conor and Srinivasan, Vivek J.},
