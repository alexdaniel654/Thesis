}

@article{majumdar_errors_1986,
  title = {Errors in the Measurements of {{T2}} Using Multiple-Echo {{MRI}} Techniques. {{I}}. {{Effects}} of Radiofrequency Pulse Imperfections},
  author = {Majumdar, S. and Orphanoudakis, S. C. and Gmitro, A. and O'Donnell, M. and Gore, J. C.},
