  shorttitle = {Diffusion Tensor Imaging of Kidneys with Respiratory Triggering},
  author = {Kataoka, Masako and Kido, Aki and Yamamoto, Akira and Nakamoto, Yuji and Koyama, Takashi and Isoda, Hiroyoshi and Maetani, Yoji and Umeoka, Shigeaki and Tamai, Ken and Saga, Tsuneo and Morisawa, Nobuko and Mori, Susumu and Togashi, Kaori},
