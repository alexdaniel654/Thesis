  journaltitle = {Ultrasound Quarterly},
  volume = {28},
  pages = {275},
  doi = {10.1097/RUQ.0b013e318276f12b},
  abstract = {Aim~Ultrasound tissue characterization (USTC) is a precursor of ultrasound virtual histology (USVH), already applied to B-mode images of coronary, carotid, and peripheral arteries, as well as venous thrombosis. Elevated echogenicity has been described for a rejected transplanted kidney. We analyzed data from healthy young adults as reference for further renal USTC.
        Methods~Ultrasound kidney images of 10 volunteers were analyzed. Pixel brightness in the 0-to-255 range was rescaled to zero for black and 200 for fascia brightness before automatic classification into 14 ranges, including ``blood-like'' (0\textendash{}4), ``fat-like'' (8\textendash{}26), ``hypoechoic muscle-like'' (41\textendash{}60), ``hyperechoic muscle-like'' (61\textendash{}76), 4 ranges of ``fiber-like'' (112\textendash{}196), ``calcium-like'' (211\textendash{}255) and intermediary intervals. Nomenclature was readapted using nonechoic, hypoechoic I to IV, echoic I to IV, hyperechoic I to IV, and saturated echoes to avoid inference to actual kidney tissue. Descriptive and comparative statistics were based on percentages of pixels in specific brightness ranges.
        Sample Population~Eight women and 2 men, 26 {$\pm$} 4 years (range, 22\textendash{}34 years) old, were studied. Kidney length was 10.5 {$\pm$} 0.9 cm (9.0\textendash{}12.0 cm). Doppler US resistivity index was 0.67 {$\pm$} 0.03 (0.62\textendash{}0.71).
        Results~Original fascia brightness converted to 200 value had a mean {$\pm$} SD of 206 {$\pm$} 16 (range, 181\textendash{}236). Kidney grayscale median averaged 37 {$\pm$} 6 (27\textendash{}48). Most pixels were hypoechoic II to IV (8\textendash{}60), averaging 78\% {$\pm$} 6\% (66\%\textendash{}87\%). Percentages for fat-like, intermediary fat/muscle-like, and hypoechoic muscle-like intervals averaged 25\%, 28\%, and 25\%, respectively.
        Conclusions~A reference database for USTC/USVH of normal young kidneys was created for future comparisons with transplanted and abnormal kidneys. Normal renal echoes have low brightness. Hyperechoic pixels may represent abnormalities.},
  file = {C\:\\Users\\ppxad2\\Zotero\\storage\\3ZUNR5PN\\Valiente Engelhorn et al. - 2012 - Ultrasound Tissue Characterization of the Normal K.pdf;C\:\\Users\\ppxad2\\Zotero\\storage\\MC9ETBZT\\Ultrasound_Tissue_Characterization_of_the_Normal.6.html},
  langid = {american},
  number = {4}
}

@article{van_den_dool_functional_2005,
  title = {Functional {{Renal Volume}}: {{Quantitative Analysis}} at {{Gadolinium}}-Enhanced {{MR Angiography}}\textemdash{{Feasibility Study}} in {{Healthy Potential Kidney Donors}}},
  shorttitle = {Functional {{Renal Volume}}},
  author = {van den Dool, Sjoerd W. and Wasser, Martin N. and de Fijter, Johan W. and Hoekstra, Jan and van der Geest, Rob J.},
