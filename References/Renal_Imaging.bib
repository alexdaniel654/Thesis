  title = {Quantitative {{BOLD}} Response of the Renal Medulla to Hyperoxic Challenge at 1.5\,{{T}} and 3.0\,{{T}}},
  author = {Donati, Olivio F. and Nanz, Daniel and Serra, Andreas L. and Boss, Andreas},
