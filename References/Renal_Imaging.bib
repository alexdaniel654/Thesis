  eprinttype = {pmid},
  file = {C\:\\Users\\ppxad2\\Zotero\\storage\\6CKZCIGY\\Friedli et al. - 2016 - New Magnetic Resonance Imaging Index for Renal Fib.pdf;C\:\\Users\\ppxad2\\Zotero\\storage\\PAJGRV3R\\Friedli et al. - 2016 - New Magnetic Resonance Imaging Index for Renal Fib.pdf;C\:\\Users\\ppxad2\\Zotero\\storage\\UXBGMW77\\Friedli et al. - 2016 - New Magnetic Resonance Imaging Index for Renal Fib.pdf;C\:\\Users\\ppxad2\\Zotero\\storage\\FD3UDEWK\\srep30088.html;C\:\\Users\\ppxad2\\Zotero\\storage\\NU46RWZW\\srep30088.html},
