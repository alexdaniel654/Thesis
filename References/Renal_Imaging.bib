  eprinttype = {pmid},
  file = {C\:\\Users\\ppxad2\\Zotero\\storage\\7JX2W67K\\Liu et al. - 2016 - Multi-site evaluations of a T2-Relaxation-Under-Sp.pdf},
  number = {2},
  pmcid = {PMC4592780}
}

