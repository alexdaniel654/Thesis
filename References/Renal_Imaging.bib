@article{haacke_vivo_1997,
  title = {In Vivo Measurement of Blood Oxygen Saturation Using Magnetic Resonance Imaging: {{A}} Direct Validation of the Blood Oxygen Level-Dependent Concept in Functional Brain Imaging},
