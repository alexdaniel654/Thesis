@article{hughes_effect_2004,
  title = {Effect of Inspired Oxygen on Portal and Hepatic Oxygenation: Effective Arterialization of Portal Blood by Hyperoxia},
  shorttitle = {Effect of Inspired Oxygen on Portal and Hepatic Oxygenation},
  author = {Hughes, Stephen J. and Yang, Wenxuan and Juszczak, Maciej and Jones, Gareth L. and Powis, Stephen H. and Seifalian, Alexander M. and Press, Martin},
  date = {2004},
  journaltitle = {Cell Transplantation},
  shortjournal = {Cell Transplant},
  volume = {13},
  pages = {801--808},
  issn = {0963-6897},
  abstract = {Because hypoxia may compromise the survival of intraportally transplanted pancreatic islets, we have measured portal blood flow and both portal and hepatic oxygenation in normal and diabetic rats breathing graded inspired oxygen concentrations. Portal blood flow and hepatic tissue oxygenation were measured using a transonic flowmeter and near infrared spectroscopy while gas analysis was carried out on portal venous blood samples. The effects of breathing 13\%, 21\%, 50\%, or 100\% oxygen were compared in animals with steptozotocin-induced diabetes and in controls. In diabetic rats breathing 21\% oxygen, portal blood flow was significantly lower than in controls (7.2+/-0.7 vs. 9.1+/-0.8 ml/min, p {$<$} 0.05). In both groups, breathing 100\% oxygen significantly increased portal flow (to 8.4+/-1.0 and 12.2+/-0.7 ml/min, respectively). This effect was not secondary to hepatic arterial vasoconstriction because it was not prevented by hepatic artery ligation. In controls, breathing 100\% oxygen increased portal pO2 from 5.0+/-0.9 to 14.4+/-1.4 kPa (p {$<$} 0.05) and portal venous oxygen saturation (PSaO2) from 53.9+/-12.1\% to 92.9+/-1.4\% (p {$<$} 0.05), a value not significantly different from peripheral (arterial) saturation. Similarly, in diabetic animals pO2 rose from 5.6+/-0.3 to 11.7+/-0.4 kPa (p {$<$} 0.01) and SO2 from 55.5+/-5.2\% to 88.5+/-0.6\% (p {$<$} 0.05). Hepatic oxyhemoglobin rose and deoxyhemoglobin fell reciprocally as a function of the inspired oxygen concentration. Improved hepatic oxygenation observed in animals breathing oxygen-enriched gas mixtures results from an increase in splanchnic blood flow coupled with a marked increase in portal oxygen saturation. This effective arterialization of portal blood may have important consequences for the success of intraportal transplantation of pancreatic islets.},
  eprint = {15690982},
  eprinttype = {pmid},
  keywords = {Animals,Blood Gas Analysis,Diabetes Mellitus; Experimental,Disease Models; Animal,Dose-Response Relationship; Drug,Hemoglobins,Hepatic Artery,Hyperoxia,hypoxia,Ligation,Liver,Liver Circulation,Male,Oxygen,Oxygen Consumption,Oxyhemoglobins,Portal Vein,Rats,Rats; Inbred Lew},
  langid = {english},
  number = {7-8}
}

@article{jain_cerebral_2014,
  title = {Cerebral {{Oxygen Metabolism}} in {{Neonates}} with {{Congenital Heart Disease Quantified}} by {{MRI}} and {{Optics}}},
  author = {Jain, Varsha and Buckley, Erin M and Licht, Daniel J and Lynch, Jennifer M and Schwab, Peter J and Naim, Maryam Y and Lavin, Natasha A and Nicolson, Susan C and Montenegro, Lisa M and Yodh, Arjun G and Wehrli, Felix W},
  date = {2014-03-01},
  journaltitle = {Journal of Cerebral Blood Flow \& Metabolism},
  shortjournal = {Journal of Cerebral Blood Flow \& Metabolism},
  volume = {34},
  pages = {380--388},
  issn = {0271-678X},
  doi = {10.1038/jcbfm.2013.214},
  abstract = {Neonatal congenital heart disease (CHD) is associated with altered cerebral hemodynamics and increased risk of brain injury. Two novel noninvasive techniques, magnetic resonance imaging (MRI) and diffuse optical and correlation spectroscopies (diffuse optical spectroscopy (DOS), diffuse correlation spectroscopy (DCS)), were employed to quantify cerebral blood flow (CBF) and oxygen metabolism (CMRO2) of 32 anesthetized CHD neonates at rest and during hypercapnia. Cerebral venous oxygen saturation (SvO2) and CBF were measured simultaneously with MRI in the superior sagittal sinus, yielding global oxygen extraction fraction (OEF) and global CMRO2 in physiologic units. In addition, microvascular tissue oxygenation (StO2) and indices of microvascular CBF (BFI) and CMRO2 (CMRO2i) in the frontal cortex were determined by DOS/DCS. Median resting-state MRI-measured OEF, CBF, and CMRO2 were 0.38, 9.7\,mL/minute per 100\,g and 0.52\,mL O2/minute per 100\,g, respectively. These CBF and CMRO2 values are lower than literature reports for healthy term neonates (which are sparse and quantified using different methods) and resemble values reported for premature infants. Comparison of MRI measurements of global SvO2, CBF, and CMRO2 with corresponding local DOS/DCS measurements demonstrated strong linear correlations (R2=0.69, 0.67, 0.67; P{$<$}0.001), permitting calibration of DOS/DCS indices. The results suggest that MRI and optics offer new tools to evaluate cerebral hemodynamics and metabolism in CHD neonates.},
  file = {C\:\\Users\\ppxad2\\Zotero\\storage\\FXVURUTV\\Jain et al. - 2014 - Cerebral Oxygen Metabolism in Neonates with Congen.pdf},
  langid = {english},
  number = {3}
}

