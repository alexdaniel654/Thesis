@article{lu_calibration_2012,
  title = {Calibration and Validation of {{TRUST MRI}} for the Estimation of Cerebral Blood Oxygenation},
  author = {Lu, Hanzhang and Xu, Feng and Grgac, Ksenija and Liu, Peiying and Qin, Qin and van Zijl, Peter},
