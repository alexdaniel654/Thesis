}

@article{coulam_measurement_2002,
  title = {Measurement of Renal Volumes with Contrast-Enhanced {{MRI}}},
  author = {Coulam, Curtis H. and Bouley, Donna M. and Sommer, F. Graham},
  date = {2002},
  journaltitle = {Journal of Magnetic Resonance Imaging},
