@article{dekkers_consensus-based_2019,
  title = {Consensus-Based Technical Recommendations for Clinical Translation of Renal {{T1}} and {{T2}} Mapping {{MRI}}},
  author = {Dekkers, Ilona A. and de Boer, Anneloes and Sharma, Kaniska and Cox, Eleanor F. and Lamb, Hildo J. and Buckley, David L. and Bane, Octavia and Morris, David M. and Prasad, Pottumarthi V. and Semple, Scott I. K. and Gillis, Keith A. and Hockings, Paul and Buchanan, Charlotte and Wolf, Marcos and Laustsen, Christoffer and Leiner, Tim and Haddock, Bryan and Hoogduin, Johannes M. and Pullens, Pim and Sourbron, Steven and Francis, Susan},
