@article{jones_imaging_2002,
  title = {Imaging the Changes in Renal {{T1}} Induced by the Inhalation of Pure Oxygen: {{A}} Feasibility Study},
  shorttitle = {Imaging the Changes in Renal {{T1}} Induced by the Inhalation of Pure Oxygen},
