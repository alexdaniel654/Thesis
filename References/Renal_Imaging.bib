  shorttitle = {A {{Novel Unsupervised Segmentation Approach Quantifies Tumor Tissue Populations Using Multiparametric MRI}}},
  author = {Katiyar, Prateek and Divine, Mathew R. and Kohlhofer, Ursula and Quintanilla-Martinez, Leticia and Sch\"olkopf, Bernhard and Pichler, Bernd J. and Disselhorst, Jonathan A.},
