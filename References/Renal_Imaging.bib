@article{constantinides_human_2000,
  title = {Human {{Skeletal Muscle}}: {{Sodium MR Imaging}} and {{Quantification}}\textemdash{{Potential Applications}} in {{Exercise}} and {{Disease}}},
  shorttitle = {Human {{Skeletal Muscle}}},
  author = {Constantinides, Chris D. and Gillen, Joseph S. and Boada, Fernando E. and Pomper, Martin G. and Bottomley, Paul A.},
  date = {2000-08-01},
  journaltitle = {Radiology},
  shortjournal = {Radiology},
  volume = {216},
